{
  "problemMatchers": [
    {
      "owner": "jekyll",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^\\s*Liquid Warning: Liquid (?:syntax )?error \\(line (\\d+)\\): (.+) in (.+)$",
          "line": 1,
          "message": 2,
          "file": 3
        }
      ]
    },
    {
      "owner": "jekyll",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^\\s*Liquid Exception: Liquid (?:syntax )?error \\(line (\\d+)\\): (.+) in (.+)$",
          "line": 1,
          "message": 2,
          "file": 3
        }
      ]
    }
  ]
}
