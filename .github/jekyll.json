{
  "problemMatcher": [
    {
      "owner": "jekyll-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(?:\\x1B\\[31m)?\\s*Liquid Warning: Liquid (?:syntax )?error \\(line (\\d+)\\): (.+) in (..*?)(?:\\x1B\\[0m)?$",
          "line": 1,
          "message": 2,
          "file": 3
        }
      ]
    },
    {
      "owner": "jekyll-error",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(?:\\x1B\\[31m)?\\s*Liquid Exception: Liquid (?:syntax )?error \\(line (\\d+)\\): (.+) in (..*?)(?:\\x1B\\[0m)?$",
          "line": 1,
          "message": 2,
          "file": 3
        }
      ]
    }
  ]
}
